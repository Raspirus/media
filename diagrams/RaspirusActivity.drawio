        <mxCell id="N5nYzkoGLXx9K-kxQQ8S-68" value="No Virus found" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" parent="1" vertex="1">
